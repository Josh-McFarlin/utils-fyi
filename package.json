{
  "name": "utils-fyi",
  "author": "Josh McFarlin <josh@mcfarl.in>",
  "license": "MIT",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Josh-McFarlin/utils-fyi.git"
  },
  "bugs": {
    "url": "https://github.com/Josh-McFarlin/utils-fyi/issues"
  },
  "homepage": "https://utils.fyi",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build && next export",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write **/*.{js,ts,tsx,test.tsx}",
    "lint": "eslint --fix --ext ts --ext tsx --ext js .",
    "test": "jest",
    "test-all": "npm run lint && npm run type-check && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "next": "latest",
    "next-images": "^1.4.0",
    "next-offline": "^5.0.2",
    "next-seo": "^4.7.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.9"
  },
  "devDependencies": {
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.27",
    "@types/testing-library__react": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-jest": "^25.2.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "jest": "^25.2.3",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.0.10",
    "prettier": "2.0.5",
    "prettier-eslint": "^11.0.0",
    "typescript": "^3.8.3"
  }
}
